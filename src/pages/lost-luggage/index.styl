::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}
page{
    background: #eeeeee;
}
.lost-luggage{
    background: #eeeeee; 
    position: relative;
}
.lost-luggage .at-tabs {
    padding-bottom: 98px !important;
    background: #eeeeee; 
}
.gsCount,
.zlCount{
    position: absolute;
    z-index: 9999; 
}
.gsCount{
    top:172px;  
    left :159px;
    font-size: 20px;
    color: #c1bcbc; 
}
.zlCount{
    top:172px;  
    left :540px; 
    font-size: 20px;
    color: #c1bcbc; 
}
.lost-luggage .at-tabs__header{
    box-shadow: 0px 3px 7px 0px rgba(0, 0, 0, 0.15);
    z-index: 999;
    background-image: url('data:image/png;base64,');
    background-size: auto 100%;    
}
.lost-luggage .at-tabs-pane{
    min-height: 70vh;   
}
.lost-luggage .at-tabs__item{
    font-family: PingFang-SC-Medium;
	font-size: 36px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #c1bcbc; 
    border-right: 3px solid rgba(211,211,211,0.5);   
}
.lost-luggage .at-tabs__item:after{
    background-color: transparent ;    
}
.lost-luggage .at-tabs__item:last-of-type{
    border-right: none; 
}
.lost-luggage .at-tabs__item--active{
    color: #7fc6e7; 
    font-weight: bold;  
}
.m-scroll-view{
    padding-bottom: 98px; 
}
.tips{
    padding-top:100px;
}
.nothing{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height :500px;    
}