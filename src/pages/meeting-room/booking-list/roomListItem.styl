.roomItem {
  margin: 0
  padding: 0
  width: 100%
  height: 182px;
  background-color: white
  margin-top 8px;
  position: relative
  color: #333
}

.header {
  height: 72px
  line-height: 72px
  font-size: 32px
  display flex
  flex-flow row nowrap
  align-items center
  letter-spacing 4px
}
.header .header-title{
  flex: 1;
  overflow :hidden;
  text-overflow: ellipsis;
  white-space :nowrap;
}
.roomIcon {
  width: 40px;
  height: 40px;
  margin: 0 28px;
}

.body {
  font-size 28px;
  margin-left 86px;
  margin-bottom 16px;
}

.footer {
  margin-left: 86px;
  color: #999
}

.arrow {
  width 18px;
  height 32px
  background-image url("../../../assets/images/arrow_right.png")
  background-size 18px 32px
  background-repeat no-repeat
  margin-left 10px
}

.auditPanel {
  font-size 28px
  color: #F88144
  position absolute
  width 24%
  height 100%
  right 30px
  bottom: 0px
  display flex
  justify-content flex-end
  align-items center
}
