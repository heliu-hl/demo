@import "../../../assets/css/variables/default.styl"
.index {
  height: 100%
  width: 100%
  display flex;
  flex-direction column;
  background-color: $backgroundColor;
}

.index-container {
  margin: 0;
  padding: 20px 0;
  width: 100%;
  background-color white
}

.header {
  display: flex
  flex-flow row nowrap
  align-items center
  height: 90px;
  width: 90%;
  border-radius 8px;
  background-color #3F8EE5
  color white
  justify-content flex-start
  margin 0 auto 36px
}

.arrow-container {
  width 58px;
  height: 90px
  line-height 90px
  text-align center
}

.header-arrow {
  width 18px;
  height: 36px;
}

.header-time {
  flex 1;
  font-size 36px;
  text-align center
}

.item-container {
  width: 96%;
  margin: 0 auto;
  height: 72px;
  line-height: 72px;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  color #171F24

}

.item-room {
  width: 96%;
  margin: 0 auto;
  height: 72px;
  line-height: 30px;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  color #171F24
}

.item-first {
  line-height 64px
}

.item-time {
  text-align: left;
  color #999999
}

.item-text {
  flex: 1;
  height: 100%;
  text-align: center;
  border-bottom: 1px solid #D7DDE1;
  font-size 28px
}

