.index {
  margin 0
  padding 0
  background-color: #F6F7FB;
  width 100%
  height 100%
  display flex
  flex-flow column nowrap
}

.header {
  width: 82%;
  height 6%
  margin 50px auto
  display flex;
  flex-flow row nowrap
  font-size 48px
  justify-content:space-between
}
  .header-invisible{
    width: 82%;
    height 6%
    margin 50px auto
    display flex;
    flex-flow row nowrap
    font-size 48px
    justify-content:space-between
    visibility hidden
  }
.item-name{
  color #333;
  overflow: hidden;
  white-space :nowrap;
  text-overflow: ellipsis;
  width: 60%;
}
.item-login-out {
  flex: 1;
  color: #2987FF;
  width: 40%;
  text-align: right;
}

.item-background {
  width: 82%
  margin 0 auto
  display flex;
  flex-flow column nowrap;
  justify-content center;
  background-repeat no-repeat
  background-size 100% 100%
  height 34%;
  padding-left 6%;
  font-size 32px
  color: #fff
}

.bg-meeting-room {
  background-image: url('../../assets/images/bg_meeting_room.jpg')

}

.bg-reservation {
  margin-top 8%;
  background-image: url('../../assets/images/bg_room_reserve.jpg')
}

.text-big {
  margin-top 14px;
  font-size 48px
}
